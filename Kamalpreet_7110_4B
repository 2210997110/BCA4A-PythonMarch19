matrix = [
    [1, 2, 3],
    [4, 5, 6],
    [7, 8, 9]
]

element_00 = matrix[0][0]
element_01 = matrix[0][1]
element_02 = matrix[0][2]

element_10 = matrix[1][0]
element_11 = matrix[1][1]
element_12 = matrix[1][2]

element_20 = matrix[2][0]
element_21 = matrix[2][1]
element_22 = matrix[2][2]

print("Element at (0, 0):", element_00)
print("Element at (0, 1):", element_01)
print("Element at (0, 2):", element_02)

print("Element at (1, 0):", element_10)
print("Element at (1, 1):", element_11)
print("Element at (1, 2):", element_12)

print("Element at (2, 0):", element_20)
print("Element at (2, 1):", element_21)
print("Element at (2, 2):", element_22)





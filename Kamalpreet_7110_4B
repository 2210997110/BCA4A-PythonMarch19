                                                                                             (Ques-4.6)
name="Kamalpreet"
roll=2210997110
print(name)
print(roll)

n = int(input("Enter the value of n: "))
first_term = 0
second_term = 1

if n <= 0:
    print("Please enter a positive integer.")
elif n == 1:
    print("Fibonacci sequence up to the", n, "term:")
    print(first_term)
else:
    print("Fibonacci sequence up to the", n, "term:")
    print(first_term)
    print(second_term)

    for i in range(2, n):
        next_term = first_term + second_term
        print(next_term)
        first_term = second_term
        second_term = next_term




